#include <iostream>
using namespace std;

int inventory[64] = {0};
int score = 0;


void getItem(int itemId, int cnt = 1, int sc = 0){
    inventory[itemId] += cnt;
    score += sc;
} //매개변수 입력 받으면 사용 아니면 초기화 된 값 사용
    
int main() {
    getItem(6, 5);
    getItem(3, 2);
    getItem(3);
    getItem(11, 34, 70000);
    
    cout << score << endl;
    
    for(int i = 0; i < 16; i++){
        cout << inventory[i] << ' ';
    }
    cout << endl;
}
