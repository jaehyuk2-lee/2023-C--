#include <iostream>

using namespace std;

int inventory[64] ={0};
int score = 0;


void getItem(int itemId, int cnt=1, int sc=0){
    inventory[itemId]+=cnt;
    score += sc;
} /*매개변수에 기본 값을 지정해 주면
   입력 받지 않을 때 기본값으로 함수를 실행한다.
   오른쪽 부터 인식하기 때문에 설정면 오른쪽부터 해줘야함*/
    
int main()
{
    getItem(6, 5);
    getItem(3, 2);
    getItem(3);
    getItem(11, 34, 70000);
    
    cout<<score<<endl;
    
    for(int i=0; i<16; i++){
        cout << inventory[i] << ' ';
    }
    cout<<endl;
}
