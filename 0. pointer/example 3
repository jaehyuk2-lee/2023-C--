/* 두개의 문자열 s1, s2를 입력 받고,
s1과 s2에 같은 index에 있는 문자들을 비교해
같으면 O 다르면 X*/

#include <iostream>
#include <cstring>
using namespace std;

int main() {
    char s1[30], s2[30];
    cin >> s1;
    cin >> s2;
    int max_len = 0;
    
    if(strlen(s1) > strlen(s2)){
        max_len = strlen(s1);
    }
    else max_len = strlen(s2);
    
    char *ps1 = s1;
    char *ps2 = s2;
    
    cout << *ps1 + 4 << endl;
    cout << *ps2 + 4 << endl;
    
    for(int i = 0; i < max_len; i++){
        if(*(ps1+i) == *(ps2+i)) cout << 'O';
        else cout << 'X';
    }
}
