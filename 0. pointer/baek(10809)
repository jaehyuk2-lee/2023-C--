#include <iostream>
using namespace std;

int main(){
    int arr[26];
    char alp[26];
    for(int i = 0; i < 26; i++){
        arr[i] = -1;
        alp[i] = 'a'+i;
        
    }
    
    
    string str;
    cin >> str;
    
    int cnt = 0;
    for(int i = 0; str[i]; i++){
        for(int j = 0; j < 26; j++){
            if(str[i] == alp[j]){
                if(arr[j] == -1){
                    arr[j] = cnt;
                    cnt++;
                }
                else cnt++;
            }
        }
    }
    
    for(int i = 0; i <26; i++){
        cout << arr[i] << ' ';
    }
}
